{
  "name": "api-v1-auth",
  "version": "0.0.2",
  "description": "Reference architecture to demonstrate ApiGtwy v1 auth",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf .build && npx tsc --noEmit",
    "deploy": "npx sls deploy --conceal",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint '**/*.{ts,tsx}' --fix",
    "prepare": "husky install",
    "prettier": "npx prettier --write .",
    "test": "npm run build && npm run lint && npm run test:unit",
    "test:e2e": "npx jest e2e.test --config jest.config.e2e.js --silent",
    "test:int": "npx jest int.test --config jest.config.int.js --silent",
    "test:unit": "npx jest unit.test --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sls-reference-architectures/api-v1-auth.git"
  },
  "author": "Seth Orell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sls-reference-architectures/api-v1-auth/issues"
  },
  "homepage": "https://github.com/sls-reference-architectures/api-v1-auth#readme",
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "^3.427.0",
    "@aws-sdk/client-cloudformation": "^3.504.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.502.0",
    "@swc/core": "^1.3.92",
    "@swc/jest": "^0.2.29",
    "@tsconfig/node16": "1.0.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/aws-lambda": "^8.10.125",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.3",
    "@types/qs": "^6.9.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "axios": "^1.6.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.1",
    "prettier": "^3.0.3",
    "qs": "^6.11.2",
    "serverless": "^3.35.2",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-esbuild": "^1.48.5",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-prune-plugin": "^2.0.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,md,ts}": "prettier --write --ignore-path .gitignore"
  },
  "dependencies": {
    "@dazn/lambda-powertools-logger": "^1.28.1",
    "@middy/core": "^4.6.5",
    "@middy/http-json-body-parser": "^4.6.5",
    "aws-jwt-verify": "^4.0.0",
    "jsonwebtoken": "^9.0.2"
  }
}
